<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="ControlBorderBrushColor">#88FFFFFF</Color>
    <Color x:Key="ControlBackgroundColor">#FF202020</Color>
    <SolidColorBrush x:Key="ControlBorderBrush"
                     Color="{StaticResource ControlBorderBrushColor}" />
    <SolidColorBrush x:Key="ControlBackground"
                     Color="{StaticResource ControlBackgroundColor}" />
</ResourceDictionary>